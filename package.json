{
    "name": "alohadot",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "rm -rf ./node_modules/.vite && unlink yarn.lock && yarn update && yarn install && yarn start",
        "update": "npx ncu -u",
        "lint": "npx eslint .",
        "fix": "npx eslint --fix -c ./.eslintrc.json ./src",
        "format": "npx prettier --write ./src --config ./.prettierrc.json",
        "start": "vite --host",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prepare": "npx huskier-init && husky install",
        "tsc": "npx tsc --noEmit"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-free": "^6.6.0",
        "@heroicons/react": "^2.1.5",
        "@mantine/core": "^7.12.1",
        "@mantine/hooks": "^7.12.1",
        "@mongez/cache": "^1.2.4",
        "@mongez/config": "^1.0.26",
        "@mongez/dom": "^1.1.2",
        "@mongez/events": "^2.1.0",
        "@mongez/http": "^2.2.8",
        "@mongez/huskier": "^2.0.0",
        "@mongez/localization": "^2.2.4",
        "@mongez/react": "^3.0.6",
        "@mongez/react-atom": "^5.0.0",
        "@mongez/react-form": "^2.1.3",
        "@mongez/react-helmet": "^1.0.9",
        "@mongez/react-hooks": "^1.3.3",
        "@mongez/react-localization": "^1.0.5",
        "@mongez/react-router": "^2.3.8",
        "@mongez/reinforcements": "^2.3.10",
        "@mongez/supportive-is": "^2.0.3",
        "@mongez/user": "^1.0.13",
        "@mui/icons-material": "^5.16.6",
        "@mui/joy": "^5.0.0-beta.48",
        "@mui/material": "^5.16.6",
        "font-awesome": "^4.7.0",
        "formik": "^2.4.6",
        "framer-motion": "^11.3.28",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-headroom": "^3.2.1",
        "react-icons": "^5.2.1",
        "react-intersection-observer": "^9.13.0",
        "react-scroll-progress-bar": "^2.0.3",
        "react-slick": "^0.30.2",
        "sass": "^1.66.1",
        "slick-carousel": "^1.8.1",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@mongez/vite": "^2.0.4",
        "@types/crypto-js": "^4.2.2",
        "@types/node": "^20.14.6",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "npm-check-updates": "^16.14.20",
        "postcss": "^8.4.41",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.3.2",
        "prettier-plugin-organize-imports": "^3.2.4",
        "tailwindcss": "^3.4.4",
        "typescript": "^5.4.5",
        "vite": "^5.3.5",
        "vite-plugin-svgr": "^4.2.0",
        "husky": "^8.0.0"
    },
    "huskier": {
        "hooks": {
            "pre-commit": [
                "yarn format",
                "yarn fix --max-warnings 0",
                "yarn tsc"
            ]
        }
    }
}
